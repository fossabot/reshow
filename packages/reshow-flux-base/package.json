{
  "version": "0.16.14",
  "name": "reshow-flux-base",
  "description": "Pure flux dispatch mechanism",
  "main": "./build/cjs/index.js",
  "module": "./build/es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-atomic/reshow",
    "directory": "packages/reshow-flux-base"
  },
  "homepage": "https://github.com/react-atomic/reshow/tree/main/packages/reshow-flux-base",
  "keywords": [
    "state",
    "global-state",
    "state-management",
    "flux",
    "reducer",
    "reshow-flux"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "call-func": "^0.1.5",
    "reshow-constant": "^0.16.0",
    "reshow-runtime": "^0.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "reshow-unit": "^0.16.0"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
